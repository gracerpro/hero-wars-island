{
  "name": "hero-wars-island-website",
  "version": "1.3.4",
  "private": false,
  "type": "module",
  "description": "The Hero Wars islands.",
  "scripts": {
    "dev": "node server",
    "dev-prod": "NODE_ENV=production node server",
    "generate": "vite build --outDir dist/static --ssrManifest .vite/ssr-manifest.json && npm run build:server && node prerender",
    "generate-only": "node prerender",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest .vite/ssr-manifest.json",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@sentry/vue": "^7.100.1",
    "@vue/server-renderer": "^3.4.19",
    "bootstrap": "^5.3.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "sirv": "^2.0.4",
    "vite-plugin-ejs": "^1.7.0",
    "vue": "^3.2.13",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.2",
    "vite": "^5.0.11",
    "vitest": "^1.3.1"
  }
}
